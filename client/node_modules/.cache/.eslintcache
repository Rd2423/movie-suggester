nge}\n              />\n              <input\n                placeholder=\"email\"\n                type=\"text\"\n                name=\"email\"\n                value={formState.email}\n                onChange={handleChange}\n              />\n              <input\n                type=\"text\"\n                name=\"password\"\n                value={formState.password}\n                onChange={handleChange}\n              />\n              <button type=\"submit\">Enter</button>\n            </form>\n          </div>\n          <div className=\"signupBtn\">\n            {/* <Link to=\"/login\">Login Instead</Link> */}\n          </div>\n        </div>\n      </div>\n    </>\n  );\n}\n\nexport default Signup;\n","/Users/rayandiouf/Desktop/movie-suggester/client/src/utils/auth.js",[],"/Users/rayandiouf/Desktop/movie-suggester/client/src/pages/MyMovies.js",[],"/Users/rayandiouf/Desktop/movie-suggester/client/src/utils/localStorage.js",[],{"ruleId":"67","severity":1,"message":"68","line":25,"column":11,"nodeType":"69","endLine":25,"endColumn":14},{"ruleId":"70","severity":1,"message":"71","line":1,"column":17,"nodeType":"72","messageId":"73","endLine":1,"endColumn":26},{"ruleId":"70","severity":1,"message":"74","line":1,"column":28,"nodeType":"72","messageId":"73","endLine":1,"endColumn":36},{"ruleId":"70","severity":1,"message":"75","line":5,"column":10,"nodeType":"72","messageId":"73","endLine":5,"endColumn":22},{"ruleId":"70","severity":1,"message":"76","line":5,"column":24,"nodeType":"72","messageId":"73","endLine":5,"endColumn":40},{"ruleId":"70","severity":1,"message":"77","line":9,"column":10,"nodeType":"72","messageId":"73","endLine":9,"endColumn":19},{"ruleId":"70","severity":1,"message":"78","line":10,"column":10,"nodeType":"72","messageId":"73","endLine":10,"endColumn":23},{"ruleId":"70","severity":1,"message":"79","line":10,"column":25,"nodeType":"72","messageId":"73","endLine":10,"endColumn":41},{"ruleId":"80","severity":1,"message":"81","line":48,"